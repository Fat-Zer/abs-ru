#. type: Plain text
#: abs-guide/read-r.sh:2
#, no-wrap
msgid "#!/bin/bash\n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:4
#, no-wrap
msgid "echo\n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:7
#, no-wrap
msgid ""
"echo \"Enter a string terminated by a \\\\, then press &lt;ENTER&gt;.\"\n"
"echo \"Then, enter a second string (no \\\\ this time), and again press &lt;ENTER&gt;.\"\n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:11
#, no-wrap
msgid ""
"read var1     # The \"\\\" suppresses the newline, when reading $var1.\n"
"              #     first line \\\n"
"              #     second line\n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:14
#, no-wrap
msgid ""
"echo \"var1 = $var1\"\n"
"#     var1 = first line second line\n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:17
#, no-wrap
msgid ""
"#  For each line terminated by a \"\\\"\n"
"#+ you get a prompt on the next line to continue feeding characters into var1.\n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:19
#, no-wrap
msgid "echo; echo\n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:23
#, no-wrap
msgid ""
"echo \"Enter another string terminated by a \\\\ , then press &lt;ENTER&gt;.\"\n"
"read -r var2  # The -r option causes the \"\\\" to be read literally.\n"
"              #     first line \\\n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:26
#, no-wrap
msgid ""
"echo \"var2 = $var2\"\n"
"#     var2 = first line \\\n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:28
#, no-wrap
msgid "# Data entry terminates with the first &lt;ENTER&gt;.\n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:30
#, no-wrap
msgid "echo \n"
msgstr ""

#. type: Plain text
#: abs-guide/read-r.sh:31
#, no-wrap
msgid "exit 0\n"
msgstr ""
