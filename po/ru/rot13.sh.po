#. type: Plain text
#: abs-guide/rot13.sh:5
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# rot13.sh: Classic rot13 algorithm,\n"
"#           encryption that might fool a 3-year old\n"
"#           for about 10 minutes.\n"
msgstr ""

#. type: Plain text
#: abs-guide/rot13.sh:9
#, no-wrap
msgid ""
"# Usage: ./rot13.sh filename\n"
"# or     ./rot13.sh &lt;filename\n"
"# or     ./rot13.sh and supply keyboard input (stdin)\n"
msgstr ""

#. type: Plain text
#: abs-guide/rot13.sh:13
#, no-wrap
msgid ""
"cat \"$@\" | tr 'a-zA-Z' 'n-za-mN-ZA-M'   # \"a\" goes to \"n\", \"b\" to "
"\"o\" ...\n"
"#  The   cat \"$@\"   construct\n"
"#+ permits input either from stdin or from files.\n"
msgstr ""

#. type: Plain text
#: abs-guide/rot13.sh:14
#, no-wrap
msgid "exit 0\n"
msgstr ""
