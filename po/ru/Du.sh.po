#. type: Plain text
#: abs-guide/Du.sh:3
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# Du.sh: DOS to UNIX text file converter.\n"
msgstr ""

#. type: Plain text
#: abs-guide/Du.sh:5
#, no-wrap
msgid "E_WRONGARGS=85\n"
msgstr ""

#. type: Plain text
#: abs-guide/Du.sh:11
#, no-wrap
msgid ""
"if [ -z \"$1\" ]\n"
"then\n"
"  echo \"Usage: `basename $0` filename-to-convert\"\n"
"  exit $E_WRONGARGS\n"
"fi\n"
msgstr ""

#. type: Plain text
#: abs-guide/Du.sh:13
#, no-wrap
msgid "NEWFILENAME=$1.unx\n"
msgstr ""

#. type: Plain text
#: abs-guide/Du.sh:18
#, no-wrap
msgid ""
"CR='\\015'  # Carriage return.\n"
"           # 015 is octal ASCII code for CR.\n"
"           # Lines in a DOS text file end in CR-LF.\n"
"           # Lines in a UNIX text file end in LF only.\n"
msgstr ""

#. type: Plain text
#: abs-guide/Du.sh:21
#, no-wrap
msgid ""
"tr -d $CR &lt; $1 &gt; $NEWFILENAME\n"
"# Delete CR's and write to new file.\n"
msgstr ""

#. type: Plain text
#: abs-guide/Du.sh:24
#, no-wrap
msgid ""
"echo \"Original DOS text file is \\\"$1\\\".\"\n"
"echo \"Converted UNIX text file is \\\"$NEWFILENAME\\\".\"\n"
msgstr ""

#. type: Plain text
#: abs-guide/Du.sh:26
#, no-wrap
msgid "exit 0\n"
msgstr ""

#. type: Plain text
#: abs-guide/Du.sh:29
#, no-wrap
msgid ""
"# Exercise:\n"
"# --------\n"
"# Change the above script to convert from UNIX to DOS.\n"
msgstr ""
