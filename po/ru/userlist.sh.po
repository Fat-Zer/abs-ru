#. type: Plain text
#: abs-guide/userlist.sh:3
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# userlist.sh\n"
msgstr ""

#. type: Plain text
#: abs-guide/userlist.sh:6
#, no-wrap
msgid ""
"PASSWORD_FILE=/etc/passwd\n"
"n=1           # User number\n"
msgstr ""

#. type: Plain text
#: abs-guide/userlist.sh:15
#, no-wrap
msgid ""
"for name in $(awk 'BEGIN{FS=\":\"}{print $1}' &lt; \"$PASSWORD_FILE\" )\n"
"# Field separator = :    ^^^^^^\n"
"# Print first field              ^^^^^^^^\n"
"# Get input from password file  /etc/passwd  ^^^^^^^^^^^^^^^^^\n"
"do\n"
"  echo \"USER #$n = $name\"\n"
"  let \"n += 1\"\n"
"done  \n"
msgstr ""

#. type: Plain text
#: abs-guide/userlist.sh:22
#, no-wrap
msgid ""
"# USER #1 = root\n"
"# USER #2 = bin\n"
"# USER #3 = daemon\n"
"# ...\n"
"# USER #33 = bozo\n"
msgstr ""

#. type: Plain text
#: abs-guide/userlist.sh:24
#, no-wrap
msgid "exit $?\n"
msgstr ""

#. type: Plain text
#: abs-guide/userlist.sh:29
#, no-wrap
msgid ""
