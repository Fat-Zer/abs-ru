#. type: Plain text
#: abs-guide/ex33a.sh:3
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# Using getopt\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex33a.sh:16
#, no-wrap
msgid ""
"# Try the following when invoking this script:\n"
"#   sh ex33a.sh -a\n"
"#   sh ex33a.sh -abc\n"
"#   sh ex33a.sh -a -b -c\n"
"#   sh ex33a.sh -d\n"
"#   sh ex33a.sh -dXYZ\n"
"#   sh ex33a.sh -d XYZ\n"
"#   sh ex33a.sh -abcd\n"
"#   sh ex33a.sh -abcdZ\n"
"#   sh ex33a.sh -z\n"
"#   sh ex33a.sh a\n"
"# Explain the results of each of the above.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex33a.sh:18
#, no-wrap
msgid "E_OPTERR=65\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex33a.sh:24
#, no-wrap
msgid ""
"if [ \"$#\" -eq 0 ]\n"
"then   # Script needs at least one command-line argument.\n"
"  echo \"Usage $0 -[options a,b,c]\"\n"
"  exit $E_OPTERR\n"
"fi  \n"
msgstr ""

#. type: Plain text
#: abs-guide/ex33a.sh:28
#, no-wrap
msgid ""
"set -- `getopt \"abcd:\" \"$@\"`\n"
"# Sets positional parameters to command-line arguments.\n"
"# What happens if you use \"$*\" instead of \"$@\"?\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex33a.sh:38
#, no-wrap
msgid ""
"while [ ! -z \"$1\" ]\n"
"do\n"
"  case \"$1\" in\n"
"    -a) echo \"Option \\\"a\\\"\";;\n"
"    -b) echo \"Option \\\"b\\\"\";;\n"
"    -c) echo \"Option \\\"c\\\"\";;\n"
"    -d) echo \"Option \\\"d\\\" $2\";;\n"
"     *) break;;\n"
"  esac\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex33a.sh:41
#, no-wrap
msgid ""
"  shift\n"
"done\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex33a.sh:44
#, no-wrap
msgid ""
"#  It is usually better to use the 'getopts' builtin in a script.\n"
"#  See \"ex33.sh.\"\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex33a.sh:45
#, no-wrap
msgid "exit 0\n"
msgstr ""
