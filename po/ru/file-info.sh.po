#. type: Plain text
#: abs-guide/file-info.sh:3
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# fileinfo.sh\n"
msgstr ""

#. type: Plain text
#: abs-guide/file-info.sh:11
#, no-wrap
msgid ""
"FILES=\"/usr/sbin/accept\n"
"/usr/sbin/pwck\n"
"/usr/sbin/chroot\n"
"/usr/bin/fakefile\n"
"/sbin/badblocks\n"
"/sbin/ypbind\"     # List of files you are curious about.\n"
"                  # Threw in a dummy file, /usr/bin/fakefile.\n"
msgstr ""

#. type: Plain text
#: abs-guide/file-info.sh:13
#, no-wrap
msgid "echo\n"
msgstr ""

#. type: Plain text
#: abs-guide/file-info.sh:16
#, no-wrap
msgid ""
"for file in $FILES\n"
"do\n"
msgstr ""

#. type: Plain text
#: abs-guide/file-info.sh:22
#, no-wrap
msgid ""
"  if [ ! -e \"$file\" ]       # Check if file exists.\n"
"  then\n"
"    echo \"$file does not exist.\"; echo\n"
"    continue                # On to next.\n"
"   fi\n"
msgstr ""

#. type: Plain text
#: abs-guide/file-info.sh:29
#, no-wrap
msgid ""
"  ls -l $file | awk '{ print $8 \"         file size: \" $5 }'  # Print 2 fields.\n"
"  whatis `basename $file`   # File info.\n"
"  # Note that the whatis database needs to have been set up for this to work.\n"
"  # To do this, as root run /usr/bin/makewhatis.\n"
"  echo\n"
"done  \n"
msgstr ""

#. type: Plain text
#: abs-guide/file-info.sh:30
#, no-wrap
msgid "exit 0\n"
msgstr ""
