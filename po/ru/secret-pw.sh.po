#. type: Plain text
#: abs-guide/secret-pw.sh:3
msgid "#!/bin/bash # secret-pw.sh: secret password"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:10
msgid ""
"echo echo -n \"Enter password \" read passwd echo \"password is $passwd\" "
"echo -n \"If someone had been looking over your shoulder, \" echo \"your "
"password would have been compromised.\""
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:12
msgid "echo &amp;&amp; echo # Two line-feeds in an \"and list.\""
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:18
#, no-wrap
msgid ""
"stty -echo    # Turns off screen echo.\n"
"#   May also be done with\n"
"#   read -sp passwd\n"
"#   A big Thank You to Leigh James for pointing this out.\n"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:24
msgid ""
"echo -n \"Enter password again \" read passwd echo echo \"password is "
"$passwd\" echo"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:26
#, no-wrap
msgid "stty echo     # Restores screen echo.\n"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:28
msgid "exit 0"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:29
msgid "# Do an 'info stty' for more on this useful-but-tricky command."
msgstr ""
