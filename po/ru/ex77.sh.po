msgid "# String expansion.  # Introduced with version 2 of Bash."
msgstr ""

#. type: Plain text
#: abs-guide/ex77.sh:8
msgid ""
"# Strings of the form $'xxx' #+ have the standard escaped characters "
"interpreted."
msgstr ""

#. type: Plain text
#: abs-guide/ex77.sh:18
#, no-wrap
msgid ""
"echo $'Ringing bell 3 times \\a \\a \\a'\n"
"     # May only ring once with certain terminals.\n"
"     # Or ...\n"
"     # May not ring at all, depending on terminal settings.\n"
"echo $'Three form feeds \\f \\f \\f'\n"
"echo $'10 newlines \\n\\n\\n\\n\\n\\n\\n\\n\\n\\n'\n"
"echo $'\\102\\141\\163\\150'\n"
"     #   B   a   s   h\n"
"     # Octal equivalent of characters.\n"
msgstr ""

