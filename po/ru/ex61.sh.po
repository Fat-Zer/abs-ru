#. type: Plain text
#: abs-guide/ex61.sh:2
#, no-wrap
msgid "#!/bin/bash\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:6
#, no-wrap
msgid ""
"# Arabic number to Roman numeral conversion\n"
"# Range: 0 - 200\n"
"# It's crude, but it works.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:8
#, no-wrap
msgid "# Extending the range and otherwise improving the script is left as an exercise.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:10
#, no-wrap
msgid "# Usage: roman number-to-convert\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:14
#, no-wrap
msgid ""
"LIMIT=200\n"
"E_ARG_ERR=65\n"
"E_OUT_OF_RANGE=66\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:20
#, no-wrap
msgid ""
"if [ -z \"$1\" ]\n"
"then\n"
"  echo \"Usage: `basename $0` number-to-convert\"\n"
"  exit $E_ARG_ERR\n"
"fi  \n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:27
#, no-wrap
msgid ""
"num=$1\n"
"if [ \"$num\" -gt $LIMIT ]\n"
"then\n"
"  echo \"Out of range!\"\n"
"  exit $E_OUT_OF_RANGE\n"
"fi  \n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:40
#, no-wrap
msgid ""
"to_roman ()   # Must declare function before first call to it.\n"
"{\n"
"number=$1\n"
"factor=$2\n"
"rchar=$3\n"
"let \"remainder = number - factor\"\n"
"while [ \"$remainder\" -ge 0 ]\n"
"do\n"
"  echo -n $rchar\n"
"  let \"number -= factor\"\n"
"  let \"remainder = number - factor\"\n"
"done  \n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:49
#, no-wrap
msgid ""
"return $number\n"
"       # Exercises:\n"
"       # ---------\n"
"       # 1) Explain how this function works.\n"
"       #    Hint: division by successive subtraction.\n"
"       # 2) Extend to range of the function.\n"
"       #    Hint: use \"echo\" and command-substitution capture.\n"
"}\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:70
#, no-wrap
msgid ""
"to_roman $num 100 C\n"
"num=$?\n"
"to_roman $num 90 LXXXX\n"
"num=$?\n"
"to_roman $num 50 L\n"
"num=$?\n"
"to_roman $num 40 XL\n"
"num=$?\n"
"to_roman $num 10 X\n"
"num=$?\n"
"to_roman $num 9 IX\n"
"num=$?\n"
"to_roman $num 5 V\n"
"num=$?\n"
"to_roman $num 4 IV\n"
"num=$?\n"
"to_roman $num 1 I\n"
"# Successive calls to conversion function!\n"
"# Is this really necessary??? Can it be simplified?\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:72
#, no-wrap
msgid "echo\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex61.sh:73
#, no-wrap
msgid "exit\n"
msgstr ""
