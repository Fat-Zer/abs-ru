#. type: Plain text
#: abs-guide/rot14.sh:4
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# A version of \"rot13\" using 'eval'.\n"
"# Compare to \"rot13.sh\" example.\n"
msgstr ""

#. type: Plain text
#: abs-guide/rot14.sh:10
#, no-wrap
msgid ""
"setvar_rot_13()              # \"rot13\" scrambling\n"
"{\n"
"  local varname=$1 varvalue=$2\n"
"  eval $varname='$(echo \"$varvalue\" | tr a-z n-za-m)'\n"
"}\n"
msgstr ""

#. type: Plain text
#: abs-guide/rot14.sh:14
#, no-wrap
msgid ""
"setvar_rot_13 var \"foobar\"   # Run \"foobar\" through rot13.\n"
"echo $var                    # sbbone\n"
msgstr ""

#. type: Plain text
#: abs-guide/rot14.sh:18
#, no-wrap
msgid ""
"setvar_rot_13 var \"$var\"     # Run \"sbbone\" through rot13.\n"
"                             # Back to original variable.\n"
"echo $var                    # foobar\n"
msgstr ""

#. type: Plain text
#: abs-guide/rot14.sh:21
#, no-wrap
msgid ""
"# This example by Stephane Chazelas.\n"
"# Modified by document author.\n"
msgstr ""

