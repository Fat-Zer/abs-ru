#. type: Plain text
#: abs-guide/ifs-empty.sh:2
#, no-wrap
msgid "#!/bin/bash\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs-empty.sh:5
#, no-wrap
msgid ""
"#  If $IFS set, but empty,\n"
"#+ then \"$*\" and \"$@\" do not echo positional params as expected.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs-empty.sh:10
#, no-wrap
msgid ""
"mecho ()       # Echo positional parameters.\n"
"{\n"
"echo \"$1,$2,$3\";\n"
"}\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs-empty.sh:14
#, no-wrap
msgid ""
"IFS=\"\"         # Set, but empty.\n"
"set a b c      # Positional parameters.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs-empty.sh:18
#, no-wrap
msgid ""
"mecho \"$*\"     # abc,,\n"
"#                   ^^\n"
"mecho $*       # a,b,c\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs-empty.sh:21
#, no-wrap
msgid ""
"mecho $@       # a,b,c\n"
"mecho \"$@\"     # a,b,c\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs-empty.sh:25
#, no-wrap
msgid ""
"#  The behavior of $* and $@ when $IFS is empty depends\n"
"#+ on which Bash or sh version being run.\n"
"#  It is therefore inadvisable to depend on this \"feature\" in a script.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs-empty.sh:28
#, no-wrap
msgid "# Thanks, Stephane Chazelas.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs-empty.sh:29
#, no-wrap
msgid "exit\n"
msgstr ""
