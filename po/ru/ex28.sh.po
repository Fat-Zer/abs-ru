#. type: Plain text
#: abs-guide/ex28.sh:2
#, no-wrap
msgid "#!/bin/bash\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:4
#, no-wrap
msgid "LIMIT=19  # Upper limit\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:7
#, no-wrap
msgid ""
"echo\n"
"echo \"Printing Numbers 1 through 20 (but not 3 and 11).\"\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:9 abs-guide/ex28.sh:34
#, no-wrap
msgid "a=0\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:13
#, no-wrap
msgid ""
"while [ $a -le \"$LIMIT\" ]\n"
"do\n"
" a=$(($a+1))\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:18
#, no-wrap
msgid ""
" if [ \"$a\" -eq 3 ] || [ \"$a\" -eq 11 ]  # Excludes 3 and 11.\n"
" then\n"
"   continue      # Skip rest of this particular loop iteration.\n"
" fi\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:21
#, no-wrap
msgid ""
" echo -n \"$a \"   # This will not execute for 3 and 11.\n"
"done \n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:24
#, no-wrap
msgid ""
"# Exercise:\n"
"# Why does the loop print up to 20?\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:26
#, no-wrap
msgid "echo; echo\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:28
#, no-wrap
msgid "echo Printing Numbers 1 through 20, but something happens after 2.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:30
#, no-wrap
msgid "##################################################################\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:32
#, no-wrap
msgid "# Same loop, but substituting 'break' for 'continue'.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:38
#, no-wrap
msgid ""
"while [ \"$a\" -le \"$LIMIT\" ]\n"
"do\n"
" a=$(($a+1))\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:43
#, no-wrap
msgid ""
" if [ \"$a\" -gt 2 ]\n"
" then\n"
"   break  # Skip entire rest of loop.\n"
" fi\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:46
#, no-wrap
msgid ""
" echo -n \"$a \"\n"
"done\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:48
#, no-wrap
msgid "echo; echo; echo\n"
msgstr ""

#. type: Plain text
#: abs-guide/ex28.sh:49
#, no-wrap
msgid "exit 0\n"
msgstr ""
