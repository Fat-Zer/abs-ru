#. type: Plain text
#: abs-guide/ifs.sh:3
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# ifs.sh\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:8
#, no-wrap
msgid ""
"var1=\"a+b+c\"\n"
"var2=\"d-e-f\"\n"
"var3=\"g,h,i\"\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:14
#, no-wrap
msgid ""
"IFS=+\n"
"# The plus sign will be interpreted as a separator.\n"
"echo $var1     # a b c\n"
"echo $var2     # d-e-f\n"
"echo $var3     # g,h,i\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:16 abs-guide/ifs.sh:25 abs-guide/ifs.sh:34 abs-guide/ifs.sh:87
#, no-wrap
msgid "echo\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:23
#, no-wrap
msgid ""
"IFS=\"-\"\n"
"# The plus sign reverts to default interpretation.\n"
"# The minus sign will be interpreted as a separator.\n"
"echo $var1     # a+b+c\n"
"echo $var2     # d e f\n"
"echo $var3     # g,h,i\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:32
#, no-wrap
msgid ""
"IFS=\",\"\n"
"# The comma will be interpreted as a separator.\n"
"# The minus sign reverts to default interpretation.\n"
"echo $var1     # a+b+c\n"
"echo $var2     # d-e-f\n"
"echo $var3     # g h i\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:41
#, no-wrap
msgid ""
"IFS=\" \"\n"
"# The space character will be interpreted as a separator.\n"
"# The comma reverts to default interpretation.\n"
"echo $var1     # a+b+c\n"
"echo $var2     # d-e-f\n"
"echo $var3     # g,h,i\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:43
#, no-wrap
msgid "# ======================================================== #\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:46
#, no-wrap
msgid ""
"# However ...\n"
"# $IFS treats whitespace differently than other characters.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:54
#, no-wrap
msgid ""
"output_args_one_per_line()\n"
"{\n"
"  for arg\n"
"  do\n"
"    echo \"[$arg]\"\n"
"  done #  ^    ^   Embed within brackets, for your viewing pleasure.\n"
"}\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:57
#, no-wrap
msgid ""
"echo; echo \"IFS=\\\" \\\"\"\n"
"echo \"-------\"\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:65
#, no-wrap
msgid ""
"IFS=\" \"\n"
"var=\" a  b c   \"\n"
"#    ^ ^^   ^^^\n"
"output_args_one_per_line $var  # output_args_one_per_line `echo \" a  b c   "
"\"`\n"
"# [a]\n"
"# [b]\n"
"# [c]\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:69
#, no-wrap
msgid ""
"echo; echo \"IFS=:\"\n"
"echo \"-----\"\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:81
#, no-wrap
msgid ""
"IFS=:\n"
"var=\":a::b:c:::\"               # Same pattern as above,\n"
"#    ^ ^^   ^^^                #+ but substituting \":\" for \" \"  ...\n"
"output_args_one_per_line $var\n"
"# []\n"
"# [a]\n"
"# []\n"
"# [b]\n"
"# [c]\n"
"# []\n"
"# []\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:84
#, no-wrap
msgid ""
"# Note \"empty\" brackets.\n"
"# The same thing happens with the \"FS\" field separator in awk.\n"
msgstr ""

#. type: Plain text
#: abs-guide/ifs.sh:88
#, no-wrap
msgid "exit\n"
msgstr ""
