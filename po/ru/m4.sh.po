#. type: Plain text
#: abs-guide/m4.sh:3
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# m4.sh: Using the m4 macro processor\n"
msgstr ""

#. type: Plain text
#: abs-guide/m4.sh:9
#, no-wrap
msgid ""
"# Strings\n"
"string=abcdA01\n"
"echo \"len($string)\" | m4                            #   7\n"
"echo \"substr($string,4)\" | m4                       # A01\n"
"echo \"regexp($string,[0-1][0-1],\\&amp;Z)\" | m4      # 01Z\n"
msgstr ""

#. type: Plain text
#: abs-guide/m4.sh:14
#, no-wrap
msgid ""
"# Arithmetic\n"
"var=99\n"
"echo \"incr($var)\" | m4                              #  100\n"
"echo \"eval($var / 3)\" | m4                          #   33\n"
msgstr ""

#. type: Plain text
#: abs-guide/m4.sh:15
#, no-wrap
msgid "exit\n"
msgstr ""
