#. type: Plain text
#: abs-guide/func-cmdlinearg.sh:5
msgid ""
"#!/bin/bash # func-cmdlinearg.sh # Call this script with a command-line "
"argument, #+ something like $0 arg1."
msgstr ""

#. type: Plain text
#: abs-guide/func-cmdlinearg.sh:8
msgid "func ()"
msgstr ""

#. type: Plain text
#: abs-guide/func-cmdlinearg.sh:12
#, no-wrap
msgid ""
"{\n"
"echo \"$1\"   # Echoes first arg passed to the function.\n"
"}           # Does a command-line arg qualify?\n"
msgstr ""

#. type: Plain text
#: abs-guide/func-cmdlinearg.sh:17
msgid ""
"echo \"First call to function: no arg passed.\" echo \"See if command-line "
"arg is seen.\" func # No! Command-line arg not seen."
msgstr ""

#. type: Plain text
#: abs-guide/func-cmdlinearg.sh:23
msgid ""
"echo \"============================================================\" echo "
"echo \"Second call to function: command-line arg passed explicitly.\" func "
"$1 # Now it's seen!"
msgstr ""

#. type: Plain text
#: abs-guide/func-cmdlinearg.sh:24
msgid "exit 0"
msgstr ""
