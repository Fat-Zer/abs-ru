#. type: Plain text
#: abs-guide/factr.sh:3
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# factr.sh: Factor a number\n"
msgstr ""

#. type: Plain text
#: abs-guide/factr.sh:7
#, no-wrap
msgid ""
"MIN=2       # Will not work for number smaller than this.\n"
"E_NOARGS=85\n"
"E_TOOSMALL=86\n"
msgstr ""

#. type: Plain text
#: abs-guide/factr.sh:13
#, no-wrap
msgid ""
"if [ -z $1 ]\n"
"then\n"
"  echo \"Usage: $0 number\"\n"
"  exit $E_NOARGS\n"
"fi\n"
msgstr ""

#. type: Plain text
#: abs-guide/factr.sh:19
#, no-wrap
msgid ""
"if [ \"$1\" -lt \"$MIN\" ]\n"
"then\n"
"  echo \"Number to factor must be $MIN or greater.\"\n"
"  exit $E_TOOSMALL\n"
"fi  \n"
msgstr ""

#. type: Plain text
#: abs-guide/factr.sh:21
#, no-wrap
msgid "# Exercise: Add type checking (to reject non-integer arg).\n"
msgstr ""

#. type: Plain text
#: abs-guide/factr.sh:30
#, no-wrap
msgid ""
"echo \"Factors of $1:\"\n"
"# -------------------------------------------------------\n"
"echo  \"$1[p]s2[lip/dli%0=1dvsr]s12sid2%0=13sidvsr[dli%0=\\\n"
"1lrli2+dsi!>.]ds.xd1&lt;2\" | dc\n"
"# -------------------------------------------------------\n"
"#  Above code written by Michel Charpentier &lt;charpov@cs.unh.edu&gt;\n"
"#  (as a one-liner, here broken into two lines for display purposes).\n"
"#  Used in ABS Guide with permission (thanks!).\n"
msgstr ""

#. type: Plain text
#: abs-guide/factr.sh:32
#, no-wrap
msgid " exit\n"
msgstr ""

