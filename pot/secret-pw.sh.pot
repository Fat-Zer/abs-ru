#. type: Plain text
#: abs-guide/secret-pw.sh:3
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# secret-pw.sh: secret password\n"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:10
#, no-wrap
msgid ""
"echo\n"
"echo -n \"Enter password \"\n"
"read passwd\n"
"echo \"password is $passwd\"\n"
"echo -n \"If someone had been looking over your shoulder, \"\n"
"echo \"your password would have been compromised.\"\n"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:12
#, no-wrap
msgid "echo &amp;&amp; echo  # Two line-feeds in an \"and list.\"\n"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:18
#, no-wrap
msgid ""
"stty -echo    # Turns off screen echo.\n"
"#   May also be done with\n"
"#   read -sp passwd\n"
"#   A big Thank You to Leigh James for pointing this out.\n"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:24
#, no-wrap
msgid ""
"echo -n \"Enter password again \"\n"
"read passwd\n"
"echo\n"
"echo \"password is $passwd\"\n"
"echo\n"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:26
#, no-wrap
msgid "stty echo     # Restores screen echo.\n"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:28
#, no-wrap
msgid "exit 0\n"
msgstr ""

#. type: Plain text
#: abs-guide/secret-pw.sh:29
#, no-wrap
msgid "# Do an 'info stty' for more on this useful-but-tricky command.\n"
msgstr ""
