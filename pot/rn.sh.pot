#. type: Plain text
#: abs-guide/rn.sh:3
#, no-wrap
msgid ""
"#! /bin/bash\n"
"# rn.sh\n"
msgstr ""

#. type: Plain text
#: abs-guide/rn.sh:8
#, no-wrap
msgid ""
"# Very simpleminded filename \"rename\" utility (based on "
"\"lowercase.sh\").\n"
"#\n"
"#  The \"ren\" utility, by Vladimir Lanin (lanin@csd2.nyu.edu),\n"
"#+ does a much better job of this.\n"
msgstr ""

#. type: Plain text
#: abs-guide/rn.sh:13
#, no-wrap
msgid ""
"ARGS=2\n"
"E_BADARGS=85\n"
"ONE=1                     # For getting singular/plural right (see below).\n"
msgstr ""

#. type: Plain text
#: abs-guide/rn.sh:20
#, no-wrap
msgid ""
"if [ $# -ne \"$ARGS\" ]\n"
"then\n"
"  echo \"Usage: `basename $0` old-pattern new-pattern\"\n"
"  # As in \"rn gif jpg\", which renames all gif files in working directory "
"to jpg.\n"
"  exit $E_BADARGS\n"
"fi\n"
msgstr ""

#. type: Plain text
#: abs-guide/rn.sh:22
#, no-wrap
msgid ""
"number=0                  # Keeps track of how many files actually "
"renamed.\n"
msgstr ""

#. type: Plain text
#: abs-guide/rn.sh:34
#, no-wrap
msgid ""
"for filename in *$1*      #Traverse all matching files in directory.\n"
"do\n"
"   if [ -f \"$filename\" ]  # If finds match...\n"
"   then\n"
"     fname=`basename $filename`            # Strip off path.\n"
"     n=`echo $fname | sed -e \"s/$1/$2/\"`   # Substitute new for old in "
"filename.\n"
"     mv $fname $n                          # Rename.\n"
"     let \"number += 1\"\n"
"   fi\n"
"done   \n"
msgstr ""

#. type: Plain text
#: abs-guide/rn.sh:41
#, no-wrap
msgid ""
"if [ \"$number\" -eq \"$ONE\" ]                # For correct grammar.\n"
"then\n"
" echo \"$number file renamed.\"\n"
"else \n"
" echo \"$number files renamed.\"\n"
"fi \n"
msgstr ""

#. type: Plain text
#: abs-guide/rn.sh:43
#, no-wrap
msgid "exit $?\n"
msgstr ""

#. type: Plain text
#: abs-guide/rn.sh:48
#, no-wrap
msgid ""
