#. type: Content of: <book><part><title>
#: abs-guide/abs-guide.xml:484
msgid "Introduction"
msgstr ""

#. type: Content of: <book><part><partintro><epigraph><para>
#: abs-guide/abs-guide.xml:489
msgid "Script: <emphasis>A writing; a written document. [Obs.]</emphasis>"
msgstr ""

#. type: Content of: <book><part><partintro><epigraph><para>
#: abs-guide/abs-guide.xml:491
msgid "--<emphasis>Webster's Dictionary</emphasis>, 1913 ed."
msgstr ""

#. type: Content of: <book><part><partintro><para>
