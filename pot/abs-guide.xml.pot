msgid "<link linkend=\"dblquo\">partial quoting</link> [double quote]"
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><formalpara><para>
#: abs-guide/abs-guide.xml:1399
msgid ""
"<emphasis>\"STRING\"</emphasis> preserves (from interpretation) most of the "
"special characters within <emphasis>STRING</emphasis>. See <xref "
"linkend=\"quoting\"/>."
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><term>
#: abs-guide/abs-guide.xml:1407
msgid "<token>'</token>"
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><formalpara><title>
#: abs-guide/abs-guide.xml:1408
msgid "<link linkend=\"snglquo\">full quoting</link> [single quote]"
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><formalpara><para>
#: abs-guide/abs-guide.xml:1410
msgid ""
"<emphasis>'STRING'</emphasis> preserves all special characters within "
"<emphasis>STRING</emphasis>. This is a stronger form of quoting than "
"<emphasis>\"STRING\"</emphasis>.  See <xref linkend=\"quoting\"/>."
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><term>
#: abs-guide/abs-guide.xml:1418 abs-guide/abs-guide.xml:1466 abs-guide/abs-guide.xml:1467
