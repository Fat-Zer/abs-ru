#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:1330
msgid "<anchor id=\"dotdirectory\"/>"
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:1331
msgid ""
"When considering directory names, <firstterm>a single dot</firstterm> "
"represents the current working directory, and <firstterm>two "
"dots</firstterm> denote the parent directory."
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para><screen>
#: abs-guide/abs-guide.xml:1338
#, no-wrap
msgid ""
"<prompt>bash$ </prompt><userinput>pwd</userinput>\n"
"<computeroutput>/home/bozo/projects</computeroutput>\n"
"\n"
"<prompt>bash$ </prompt><userinput>cd .</userinput>\n"
"<prompt>bash$ </prompt><userinput>pwd</userinput>\n"
"<computeroutput>/home/bozo/projects</computeroutput>\n"
"\n"
"<prompt>bash$ </prompt><userinput>cd ..</userinput>\n"
"<prompt>bash$ </prompt><userinput>pwd</userinput>\n"
"<computeroutput>/home/bozo/</computeroutput>\n"
"\t        "
