#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:2538
msgid ""
"<userinput>scriptname >filename</userinput> redirects the output of "
"<filename>scriptname</filename> to file "
"<filename>filename</filename>. Overwrite <filename>filename</filename> if it "
"already exists."
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:2543
msgid "<anchor id=\"redirouterror\"/>"
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:2544
msgid ""
"<userinput>command &amp;>filename</userinput> redirects both the <link "
"linkend=\"stdinoutdef\"><filename>stdout</filename></link> and the "
"<filename>stderr</filename> of <filename>command</filename> to "
"<filename>filename</filename>."
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><note><para>
#: abs-guide/abs-guide.xml:2555
msgid ""
"<anchor id=\"devnullredirect\"/> This is useful for suppressing output when "
"testing for a condition. For example, let us test whether a certain command "
"exists."
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><note><para><screen>
#: abs-guide/abs-guide.xml:2563
#, no-wrap
msgid ""
"<prompt>bash$ </prompt><userinput>type bogus_command "
"&amp;>/dev/null</userinput>\n"
"<computeroutput></computeroutput>\n"
"\n"
"\n"
"<prompt>bash$ </prompt><userinput>echo $?</userinput>\n"
"<computeroutput>1</computeroutput>\n"
"                    "
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><note><para>
#: abs-guide/abs-guide.xml:2572
msgid "Or in a script:"
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><note><para><programlisting>
#: abs-guide/abs-guide.xml:2574
#, no-wrap
msgid ""
"command_test () { type \"$1\" &amp;>/dev/null; }\n"
"#                                      ^\n"
"\n"
"cmd=rmdir            # Legitimate command.\n"
"command_test $cmd; echo $?   # 0\n"
"\n"
"\n"
"cmd=bogus_command    # Illegitimate command\n"
"command_test $cmd; echo $?   # 1"
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:2587
msgid "<anchor id=\"redirouterror2\"/>"
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:2588
msgid ""
"<userinput>command >&amp;2</userinput> redirects <filename>stdout</filename> "
"of <filename>command</filename> to <filename>stderr</filename>."
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:2592
msgid ""
"<userinput>scriptname >>filename</userinput> appends the output of "
"<filename>scriptname</filename> to file <filename>filename</filename>. If "
"<filename>filename</filename> does not already exist, it is created."
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:2598
msgid "<anchor id=\"redirrw\"/>"
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:2599
msgid ""
"<userinput>[i]&lt;&gt;filename</userinput> opens file "
"<filename>filename</filename> for reading and writing, and assigns <link "
"linkend=\"fdref\">file descriptor</link> <token>i</token> to it. If "
"<filename>filename</filename> does not exist, it is created."
msgstr ""

#. type: Content of: <book><part><chapter><variablelist><varlistentry><listitem><formalpara><title>
