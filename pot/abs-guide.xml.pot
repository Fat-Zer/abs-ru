#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><term>
#: abs-guide/abs-guide.xml:9377
msgid "<anchor id=\"substrextr02\"/>${string:position:length}"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:9384
msgid ""
"Extracts <replaceable>$length</replaceable> characters of substring from "
"<replaceable>$string</replaceable> at <replaceable>$position</replaceable>."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><programlisting>
#: abs-guide/abs-guide.xml:9388
#, no-wrap
msgid ""
"stringZ=abcABC123ABCabc\n"
"#       0123456789.....\n"
"#       0-based indexing.\n"
"\n"
"echo ${stringZ:0}                            # abcABC123ABCabc\n"
"echo ${stringZ:1}                            # bcABC123ABCabc\n"
"echo ${stringZ:7}                            # 23ABCabc\n"
"\n"
"echo ${stringZ:7:3}                          # 23A\n"
"                                             # Three characters of "
"substring.\n"
"\n"
"\n"
"\n"
"# Is it possible to index from the right end of the string?\n"
"    \n"
"echo ${stringZ:-4}                           # abcABC123ABCabc\n"
"# Defaults to full string, as in ${parameter:-default}.\n"
"# However . . .\n"
"\n"
"echo ${stringZ:(-4)}                         # Cabc \n"
"echo ${stringZ: -4}                          # Cabc\n"
"# Now, it works.\n"
"# Parentheses or added space \"escape\" the position parameter.\n"
"\n"
"# Thank you, Dan Jacobson, for pointing this out."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:9415
