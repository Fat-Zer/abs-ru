#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:9479
msgid ""
"Extracts <replaceable>$substring</replaceable> at beginning of "
"<replaceable>$string</replaceable>, where "
"<replaceable>$substring</replaceable> is a <link "
"linkend=\"regexref\">regular expression</link>."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><term>
#: abs-guide/abs-guide.xml:9487
msgid "expr \"$string\" : '\\($substring\\)'"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:9495
msgid ""
"Extracts <replaceable>$substring</replaceable> at beginning of "
"<replaceable>$string</replaceable>, where "
"<replaceable>$substring</replaceable> is a regular expression."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><programlisting>
#: abs-guide/abs-guide.xml:9501
#, no-wrap
msgid ""
"stringZ=abcABC123ABCabc\n"
"#       =======\t    \n"
"\n"
"echo `expr match \"$stringZ\" '\\(.[b-c]*[A-Z]..[0-9]\\)'`   # abcABC1\n"
"echo `expr \"$stringZ\" : '\\(.[b-c]*[A-Z]..[0-9]\\)'`       # abcABC1\n"
"echo `expr \"$stringZ\" : '\\(.......\\)'`                   # abcABC1\n"
"# All of the above forms give an identical result."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><term>
#: abs-guide/abs-guide.xml:9513
msgid "expr match \"$string\" '.*\\($substring\\)'"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:9520 abs-guide/abs-guide.xml:9537
msgid ""
"Extracts <replaceable>$substring</replaceable> at <emphasis>end</emphasis> "
"of <replaceable>$string</replaceable>, where "
"<replaceable>$substring</replaceable> is a regular expression."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><term>
#: abs-guide/abs-guide.xml:9529
msgid "expr \"$string\" : '.*\\($substring\\)'"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><programlisting>
#: abs-guide/abs-guide.xml:9543
#, no-wrap
msgid ""
"stringZ=abcABC123ABCabc\n"
"#                ======\n"
"\n"
"echo `expr match \"$stringZ\" '.*\\([A-C][A-C][A-C][a-c]*\\)'`    # ABCabc\n"
"echo `expr \"$stringZ\" : '.*\\(......\\)'`                       # ABCabc"
msgstr ""

