"echo \"var1 = $var1\"   # var1 = 1\n"
"\n"
"(( var1++ ))          # x.sh: line 4: var1: readonly variable"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><term>
#: abs-guide/abs-guide.xml:8886
msgid "<token>-i</token> <replaceable>integer</replaceable>"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><programlisting>
#: abs-guide/abs-guide.xml:8890
#, no-wrap
msgid ""
"declare -i number\n"
"# The script will treat subsequent occurrences of \"number\" as an "
"integer.\t\t\n"
"\n"
"number=3\n"
"echo \"Number = $number\"     # Number = 3\n"
"\n"
"number=three\n"
