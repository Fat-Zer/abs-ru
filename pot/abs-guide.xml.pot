#, no-wrap
msgid "&scriptdetector;"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><example><title>
#: abs-guide/abs-guide.xml:16099
msgid "Generating 10-digit random numbers"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><example><programlisting>
