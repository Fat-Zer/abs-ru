#: abs-guide/abs-guide.xml:18261
#, no-wrap
msgid "&filecomp;"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><tip><para>
#: abs-guide/abs-guide.xml:18264
msgid "Use <command>zcmp</command> on <firstterm>gzipped</firstterm> files."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><term>
#: abs-guide/abs-guide.xml:18271
msgid "<anchor id=\"commref\"/><command>comm</command>"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><indexterm><secondary>
#: abs-guide/abs-guide.xml:18273 abs-guide/abs-guide.xml:18277
msgid "comm"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:18280
msgid ""
"Versatile file comparison utility. The files must be sorted for this to be "
"useful."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:18283
msgid ""
"<command>comm <replaceable>-options</replaceable> "
"<replaceable>first-file</replaceable> "
"<replaceable>second-file</replaceable></command>"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:18288
msgid "<userinput>comm file-1 file-2</userinput> outputs three columns:"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><itemizedlist><listitem><para>
#: abs-guide/abs-guide.xml:18290
msgid "column 1 = lines unique to <filename>file-1</filename>"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><itemizedlist><listitem><para>
#: abs-guide/abs-guide.xml:18293
msgid "column 2 = lines unique to <filename>file-2</filename>"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><itemizedlist><listitem><para>
#: abs-guide/abs-guide.xml:18296
msgid "column 3 = lines common to both."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:18300
msgid "The options allow suppressing output of one or more columns."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><itemizedlist><listitem><para>
#: abs-guide/abs-guide.xml:18302
msgid "<option>-1</option> suppresses column <literal>1</literal>"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><itemizedlist><listitem><para>
#: abs-guide/abs-guide.xml:18305
msgid "<option>-2</option> suppresses column <literal>2</literal>"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><itemizedlist><listitem><para>
#: abs-guide/abs-guide.xml:18308
msgid "<option>-3</option> suppresses column <literal>3</literal>"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para><itemizedlist><listitem><para>
#: abs-guide/abs-guide.xml:18311
msgid ""
"<option>-12</option> suppresses both columns <literal>1</literal> and "
"<literal>2</literal>, etc."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><variablelist><varlistentry><listitem><para>
#: abs-guide/abs-guide.xml:18317
msgid ""
"This command is useful for comparing <quote>dictionaries</quote> or "
"<firstterm>word lists</firstterm> -- sorted text files with one word per "
"line."
msgstr ""

