"<computeroutput>AUTHORCOPY=/home/bozo/posts\n"
" BASH=/bin/bash\n"
" BASH_VERSION=$'2.05.8(1)-release'\n"
" ...\n"
" XAUTHORITY=/home/bozo/.Xauthority\n"
" _=/etc/bashrc\n"
" variable22=abc\n"
" variable23=xzy</computeroutput>\n"
