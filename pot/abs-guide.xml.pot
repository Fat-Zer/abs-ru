msgid ""
"A particularly clever use of <link "
"linkend=\"testconstructs1\">if-test</link> constructs is for comment blocks."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><sect2><itemizedlist><listitem><para><programlisting>
#: abs-guide/abs-guide.xml:31966
#, no-wrap
msgid ""
"#!/bin/bash\n"
"\n"
"COMMENT_BLOCK=\n"
"#  Try setting the above variable to some value\n"
"#+ for an unpleasant surprise.\n"
"\n"
"if [ $COMMENT_BLOCK ]; then\n"
"\n"
"Comment block --\n"
"=================================\n"
"This is a comment line.\n"
"This is another comment line.\n"
"This is yet another comment line.\n"
"=================================\n"
"\n"
"echo \"This will not echo.\"\n"
"\n"
"Comment blocks are error-free! Whee!\n"
"\n"
"fi\n"
"\n"
"echo \"No more comments, please.\"\n"
"\n"
"exit 0"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><sect2><itemizedlist><listitem><para>
