#. type: Content of: <book><part><chapter><sect1><title>
#: abs-guide/abs-guide.xml:31190
msgid "Tests and Comparisons: Alternatives"
msgstr ""

#. type: Content of: <book><part><chapter><sect1><para><programlisting>
#: abs-guide/abs-guide.xml:31198
#, no-wrap
msgid ""
"a=8\n"
"\n"
"# All of the comparisons below are equivalent.\n"
"test \"$a\" -lt 16 &amp;&amp; echo \"yes, $a &lt; 16\"         # \"and "
"list\"\n"
"/bin/test \"$a\" -lt 16 &amp;&amp; echo \"yes, $a &lt; 16\" \n"
"[ \"$a\" -lt 16 ] &amp;&amp; echo \"yes, $a &lt; 16\" \n"
"[[ $a -lt 16 ]] &amp;&amp; echo \"yes, $a &lt; 16\"          # Quoting "
"variables within\n"
"(( a &lt; 16 )) &amp;&amp; echo \"yes, $a &lt; 16\"             # [[ ]] and "
"(( )) not necessary.\n"
"\n"
"city=\"New York\"\n"
"# Again, all of the comparisons below are equivalent.\n"
"test \"$city\" \\&lt; Paris &amp;&amp; echo \"Yes, Paris is greater than "
"$city\"\n"
"                                  # Greater ASCII order.\n"
"/bin/test \"$city\" \\&lt; Paris &amp;&amp; echo \"Yes, Paris is greater "
"than $city\" \n"
"[ \"$city\" \\&lt; Paris ] &amp;&amp; echo \"Yes, Paris is greater than "
"$city\" \n"
"[[ $city &lt; Paris ]] &amp;&amp; echo \"Yes, Paris is greater than "
"$city\"\n"
"                                  # Need not quote $city.\n"
"\n"
"# Thank you, S.C."
msgstr ""

#. type: Content of: <book><part><chapter><sect1><para>
#: abs-guide/abs-guide.xml:31192
msgid ""
"For tests, the <link linkend=\"dblbrackets\">[[ ]]</link> construct may be "
