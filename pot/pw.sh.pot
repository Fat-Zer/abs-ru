#. type: Plain text
#: abs-guide/pw.sh:9
#, no-wrap
msgid ""
"#!/bin/bash\n"
"#\n"
"#\n"
"#  Random password generator for Bash 2.x +\n"
"#+ by Antek Sawicki &lt;tenox@tenox.tc&gt;,\n"
"#+ who generously gave usage permission to the ABS Guide author.\n"
"#\n"
"# ==> Comments added by document author ==>\n"
msgstr ""

#. type: Plain text
#: abs-guide/pw.sh:15
#, no-wrap
msgid ""
"MATRIX=\"0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz\"\n"
"# ==> Password will consist of alphanumeric characters.\n"
"LENGTH=\"8\"\n"
"# ==> May change 'LENGTH' for longer password.\n"
msgstr ""

#. type: Plain text
#: abs-guide/pw.sh:23
#, no-wrap
msgid ""
"while [ \"${n:=1}\" -le \"$LENGTH\" ]\n"
"# ==> Recall that := is \"default substitution\" operator.\n"
"# ==> So, if 'n' has not been initialized, set it to 1.\n"
"do\n"
"\tPASS=\"$PASS${MATRIX:$(($RANDOM%${#MATRIX})):1}\"\n"
"\t# ==> Very clever, but tricky.\n"
msgstr ""

#. type: Plain text
#: abs-guide/pw.sh:26
#, no-wrap
msgid ""
"\t# ==> Starting from the innermost nesting...\n"
"\t# ==> ${#MATRIX} returns length of array MATRIX.\n"
msgstr ""

#. type: Plain text
#: abs-guide/pw.sh:29
#, no-wrap
msgid ""
"\t# ==> $RANDOM%${#MATRIX} returns random number between 1\n"
"\t# ==> and [length of MATRIX] - 1.\n"
msgstr ""

#. type: Plain text
#: abs-guide/pw.sh:34
#, no-wrap
msgid ""
"\t# ==> ${MATRIX:$(($RANDOM%${#MATRIX})):1}\n"
"\t# ==> returns expansion of MATRIX at random position, by length 1. \n"
"\t# ==> See {var:pos:len} parameter substitution in Chapter 9.\n"
"\t# ==> and the associated examples.\n"
msgstr ""

#. type: Plain text
#: abs-guide/pw.sh:36
#, no-wrap
msgid ""
"\t# ==> PASS=... simply pastes this result onto previous PASS "
"(concatenation).\n"
msgstr ""

#. type: Plain text
#: abs-guide/pw.sh:41
#, no-wrap
msgid ""
"\t# ==> To visualize this more clearly, uncomment the following line\n"
"\t#                 echo \"$PASS\"\n"
"\t# ==> to see PASS being built up,\n"
"\t# ==> one character at a time, each iteration of the loop.\n"
msgstr ""

#. type: Plain text
#: abs-guide/pw.sh:45
#, no-wrap
msgid ""
"\tlet n+=1\n"
"\t# ==> Increment 'n' for next pass.\n"
"done\n"
msgstr ""

#. type: Plain text
#: abs-guide/pw.sh:47
#, no-wrap
msgid "echo \"$PASS\"      # ==> Or, redirect to a file, as desired.\n"
msgstr ""

#. type: Plain text
#: abs-guide/pw.sh:48
#, no-wrap
msgid "exit 0\n"
msgstr ""
